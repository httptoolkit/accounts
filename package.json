{
  "name": "httptoolkit-accounts",
  "version": "0.1.0",
  "description": "Netlify functions & UI powering HTTP Toolkit account services",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "npm run start:api & npm run start:ui",
    "start:api": "netlify-lambda -c api/webpack.js serve api/src/functions/ --port 2021",
    "start:ui": "webpack serve --config ui/webpack.js",
    "build": "rimraf dist/ && npm run build:api && npm run build:ui",
    "build:api": "webpack --config api/webpack.js",
    "build:ui": "webpack --config ui/webpack.js",
    "test": "npm run build && npm run test:built-api",
    "test:built-api": "env-cmd -f api/test/ts-node.env mocha -r ts-node/register api/test/**/*.spec.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/httptoolkit/accounts.git"
  },
  "author": "Tim Perry <tim@httptoolkit.tech>",
  "license": "AGPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/httptoolkit/accounts/issues"
  },
  "homepage": "https://github.com/httptoolkit/accounts#readme",
  "devDependencies": {
    "@httptoolkit/netlify-cli": "^2.19.1-testable",
    "@sentry/node": "^4.6.1",
    "@types/auth0": "^2.9.10",
    "@types/aws-lambda": "^8.10.17",
    "@types/chai": "^4.1.7",
    "@types/jsonwebtoken": "^8.3.0",
    "@types/mocha": "^5.2.5",
    "@types/node": "^14.14.22",
    "@types/node-fetch": "^2.5.2",
    "@types/stoppable": "^1.1.0",
    "auth0": "^2.27.1",
    "chai": "^4.2.0",
    "env-cmd": "^10.1.0",
    "google-fonts-plugin": "^5.0.2",
    "html-webpack-plugin": "^4.5.1",
    "jsonwebtoken": "^8.4.0",
    "mocha": "^5.2.0",
    "mockttp": "^1.0.6",
    "moment": "^2.24.0",
    "netlify-lambda": "^1.3.1",
    "node-fetch": "^2.6.1",
    "php-serialize": "^2.1.0",
    "rimraf": "^3.0.2",
    "stoppable": "^1.1.0",
    "ts-loader": "^8.0.14",
    "ts-node": "^8.4.1",
    "typescript": "^4.2.3",
    "typescript-styled-plugin": "^0.15.0",
    "webpack": "^4.46.0",
    "webpack-cli": "^4.4.0",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "@reach/router": "^1.3.4",
    "@types/lodash": "^4.14.168",
    "@types/reach__router": "^1.3.7",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/styled-components": "^5.1.9",
    "date-fns": "^2.20.3",
    "lodash": "^4.17.21",
    "mobx": "^6.0.5",
    "mobx-react-lite": "^3.1.7",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "styled-components": "^5.2.3",
    "styled-reset": "^4.3.4"
  }
}
