{
  "name": "httptoolkit-accounts",
  "version": "0.1.0",
  "description": "Netlify functions powering HTTP Toolkit account services",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "netlify-lambda -c ./webpack.js serve src/functions/ --port 2021",
    "build": "netlify-lambda -c ./webpack.js build src/functions/ && mkdir -p public/",
    "test": "npm run build && npm run test:built",
    "test:built": "NODE_EXTRA_CA_CERTS='./test/fixtures/test-ca.pem' TS_NODE_FILES=true mocha -r ts-node/register test/**/*.spec.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/httptoolkit/accounts.git"
  },
  "author": "Tim Perry <tim@httptoolkit.tech>",
  "license": "AGPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/httptoolkit/accounts/issues"
  },
  "homepage": "https://github.com/httptoolkit/accounts#readme",
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-typescript": "^7.1.0",
    "@babel/runtime": "^7.3.1",
    "@httptoolkit/netlify-cli": "^2.19.1-testable",
    "@sentry/node": "^4.6.1",
    "@types/auth0": "^2.9.10",
    "@types/aws-lambda": "^8.10.17",
    "@types/chai": "^4.1.7",
    "@types/jsonwebtoken": "^8.3.0",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.18",
    "@types/node-fetch": "^2.5.2",
    "@types/stoppable": "^1.1.0",
    "auth0": "^2.27.1",
    "chai": "^4.2.0",
    "jsonwebtoken": "^8.4.0",
    "mocha": "^5.2.0",
    "mockttp": "^0.19.0",
    "moment": "^2.24.0",
    "netlify-lambda": "^1.3.1",
    "node-fetch": "^2.6.0",
    "php-serialize": "^2.1.0",
    "stoppable": "^1.1.0",
    "ts-node": "^8.4.1"
  }
}
